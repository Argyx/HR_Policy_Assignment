# Ignore Python cache files and virtual environments
__pycache__/
*.py[cod]
.Python
env/
venv/

# Ignore Jupyter notebook checkpoints
.ipynb_checkpoints/

# Ignore compiled code and bytecode
*.o
*.out
*.class

# Ignore data and results
*.csv
*.tsv
*.xlsx

# Ignore OS-specific files
.DS_Store
Thumbs.db

# Ignore logs
*.log
*.log.*

# Ignore any files generated by your code or dependencies
output/
reports/
*.png
*.html

# Do not ignore any files in the sample_output directory, even if they match previous rules
!sample_output/
!sample_output/*.csv
!sample_output/*.tsv
!sample_output/*.xlsx
!sample_output/*.png
!sample_output/*.html
